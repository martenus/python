#nefunguje kontrola na binarnost cisla
check = True
while check == True:
  try:
     x = input("vloz cislu na konverziu do decimalnej sustavy")
     check = False
     for i in x:
       if i != '0' or i != '1':
         check = True
  
     break   

  except ValueError:
     print("zle nedobre")

x = x[::-1]

out = 0
j = 0

for i in x:
  i = int(i)
  out = out + i*2**(j)
  j = j+1

print(out)
